script/source = "extends TileSet

const ROCK = 0
const GREEN_ROCK = 1
const ORANGE_ROCK = 2
const BRICK = 3
const GREEN_BRICK = 4
const ORANGE_BRICK = 5

var binds = {
	ROCK: [GREEN_ROCK, ORANGE_ROCK, BRICK, GREEN_BRICK, ORANGE_BRICK],
	GREEN_ROCK: [ROCK, ORANGE_ROCK, BRICK, GREEN_BRICK, ORANGE_BRICK],
	ORANGE_ROCK: [ROCK, GREEN_ROCK, BRICK, GREEN_BRICK, ORANGE_BRICK],
	BRICK: [ROCK, GREEN_ROCK, ORANGE_ROCK, GREEN_BRICK, ORANGE_BRICK],
	GREEN_BRICK: [ROCK, GREEN_ROCK, ORANGE_ROCK, BRICK, ORANGE_BRICK],
	ORANGE_BRICK: [ROCK, GREEN_ROCK, ORANGE_ROCK, BRICK, GREEN_BRICK]
}

func _is_tile_bound(id, nid):
	if id in binds:
		return nid in binds[id]
	return false
"

